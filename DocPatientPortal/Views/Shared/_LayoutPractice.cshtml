<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewData["Title"]-DocPatientPortal</title>
    <!-- plugins:css -->
    <link href="~/vendors/simple-line-icons/css/simple-line-icons.css" rel="stylesheet" />
    <link href="~/vendors/flag-icon-css/css/flag-icon.min.css" rel="stylesheet" />
    <link href="~/vendors/css/vendor.bundle.base.css" rel="stylesheet" />
    <!-- endinject -->
    <!-- Plugin css for this page -->
    <link href="~/vendors/daterangepicker/daterangepicker.css" rel="stylesheet" />
    <link href="~/vendors/chartist/chartist.min.css" rel="stylesheet" />
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link href="~/lib/jquery-datatables/css/jquery.dataTables.min.css" rel="stylesheet" />
    <!-- endinject -->
    <!-- Layout styles -->
    <link href="~/c_css/style.css" rel="stylesheet" />
    <!-- End layout styles -->
    
</head>
<body>
    <div class="container-scroller">
        <!-- partial:partials/_navbar.html -->
        <nav class="navbar default-layout-navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
            <div class="navbar-brand-wrapper d-flex align-items-center">
                <a class="navbar-brand brand-logo" asp-controller="NewRequest" asp-action="Index">
                    <img src="images/logo.svg" alt="logo" class="logo-dark" />
                </a>
                <a class="navbar-brand brand-logo-mini" href="index.html"><img src="images/logo-mini.svg" alt="logo" /></a>
            </div>
            <div class="navbar-menu-wrapper d-flex align-items-center flex-grow-1">
                <h5 class="mb-0 font-weight-medium d-none d-lg-flex">Welcome Admin!</h5>
                <ul class="navbar-nav navbar-nav-right ml-auto">




                    <li class="nav-item dropdown d-none d-xl-inline-flex user-dropdown">
                        <a class="nav-link dropdown-toggle" id="UserDropdown" href="#" data-toggle="dropdown" aria-expanded="false">
                            <img class="img-xs rounded-circle ml-2" src="images/faces/face8.jpg" alt="Profile image"> <span class="font-weight-normal"> admin username </span>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right navbar-dropdown" aria-labelledby="UserDropdown">
                            <div class="dropdown-header text-center">
                                <p class="mb-1 mt-3">username</p>
                                <p class="font-weight-light text-muted mb-0">portaladmin@gmail.com</p>
                            </div>
                            <a asp-controller="Login" asp-action="Logout" class="dropdown-item"><i class="dropdown-item-icon icon-power text-primary"></i>Sign Out</a>
                        </div>
                    </li>
                </ul>
                <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
                    <span class="icon-menu"></span>
                </button>
            </div>
        </nav>
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_sidebar.html -->
            <nav class="sidebar sidebar-offcanvas" id="sidebar">
                <ul class="nav">
                    <li class="nav-item nav-profile">
                        <a href="#" class="nav-link">
                            <div class="profile-image">
                                <img class="img-xs rounded-circle" src="images/faces/face8.jpg" alt="profile image">
                                <div class="dot-indicator bg-success"></div>
                            </div>
                            <div class="text-wrapper">
                                <p class="profile-name">Dynamic Username</p>
                                <p class="designation">Administrator</p>
                            </div>

                        </a>
                    </li>
                    <li class="nav-item nav-category">
                        <span class="nav-link">Dashboard</span>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="NewRequest" asp-action="Index">
                            <span class="menu-title">New Request</span>
                            <i class="icon-screen-desktop menu-icon"></i>
                        </a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Admin_Doctors" asp-action="View_Doctor">
                            <span class="menu-title">Add/View(Doc)</span>
                            <i class="icon-screen-desktop menu-icon"></i>
                        </a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Admin_Customers" asp-action="View_Customers">
                            <span class="menu-title">View Customer</span>
                            <i class="icon-screen-desktop menu-icon"></i>
                        </a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Admin_Appointment" asp-action="View_Appointment">
                            <span class="menu-title">Appointments</span>
                            <i class="icon-screen-desktop menu-icon"></i>
                        </a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Organ" asp-action="OrgSearch">
                            <span class="menu-title">Organ Search</span>
                            <i class="icon-screen-desktop menu-icon"></i>
                        </a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Speciality" asp-action="Index">
                            <span class="menu-title">Add Speciality</span>
                            <i class="icon-screen-desktop menu-icon"></i>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Feedback" asp-action="Index">
                            <span class="menu-title">Feedbacks</span>
                            <i class="icon-screen-desktop menu-icon"></i>
                        </a>
                    </li>
                    <!--side navbar ends-->
                </ul>
            </nav>
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">


                    <!--Rendor body starts-->
                    <div class="container">
                        <main role="main" class="pb-3">
                            @RenderBody()
                        </main>
                    </div>
                    <!--Rendor body Ends-->


                </div>
                <!-- content-wrapper ends -->
                <!-- partial:partials/_footer.html -->
                <footer class="footer">
                    <div class="d-sm-flex justify-content-center justify-content-sm-between">
                        <span class="text-muted d-block text-center text-sm-left d-sm-inline-block">Copyright © pawanbastola.com.np</span>

                    </div>
                </footer>
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="./vendors/chart.js/Chart.min.js"></script>
    <script src="~/vendors/moment/moment.js"></script>
    <script src="~/vendors/daterangepicker/daterangepicker.js"></script>
    <script src="~/vendors/chartist/chartist.min.js"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="~/c_js/off-canvas.js"></script>
    <script src="~/c_js/misc.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page -->
    <script src="~/c_js/dashboard.js"></script>
    <!-- End custom js for this page -->

    <script src="~/lib/jquery-datatables/js/jquery.dataTables.min.js"></script>
    <!--Custom Asp Code Starts-->
    @RenderSection("Scripts", required: false)
    <!--Custom Asp Code End-->


</body>
</html>