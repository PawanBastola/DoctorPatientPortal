
@model DocPatientPortal.Models.User
<p>this is the view of the profile page . and the tutorial of Entity framework.</p>

<form method="post" asp-controller="Profile" asp-action="addUsers">

    Name: <input type="text" name="pname" />
    Address: <input type="text" name="paddress" />
    Blood Group: <input type="text" name="pblood" />
    <button class="btn btn-primary">Insert</button>
</form>
<br />

<table class="table table-dark table-hover">
    <tr>
        <th>ID</th>
        <th>NAME</th>
        <th>ADDRESS</th>
        <th>BLOOD GP.</th>
    </tr>
  

    @foreach (var item in ViewBag.Data)
    {

        <tr>
            <td>@item.pid</td>
            <td>@item.pname</td>
            <td>@item.paddress</td>
            <td>@item.pblood</td>

        </tr>
    }

</table>
