@page
@model DocPatientPortal.Views.Default.IndexModel
@{
    Layout = "~/Views/Default/_Layout.cshtml";
}
<h1>@ViewBag.Heading</h1>

<div>
    <ul type="none">
        @foreach (var docattrib in ViewBag.DoctorIdentity)
        {

        <li>Name : <span style="background-color:red;">@docattrib.name </span>Speciality : <span style="background-color:gray">@docattrib.speciality</span></li>         
        }
    </ul>
    
</div>